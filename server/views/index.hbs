<section id='product_list'>
    {{#each products}}
        <div class='product'>
            <img src={{this.imageUrl}} />
            <div class='details'>
                <div class='title'>{{this.name}}</div>
                <div class='price'>{{this.price}}</div>
                <div class='description'>{{this.description}}</div>
            </div>
        </div>
    {{/each}}
</section>
